{  
    "manifest_version" : 3,  
    "name" : "時雨の町 Extension",  
    "description" : "Extension :)",  
    "version" : "1.0",  
    "action" : {  
        "default_title" : "時雨の町 Extension",  
        "default_icon" : "icon.png"      
    },  
    "content_scripts" : [  
        {  
            "matches" : ["https://www.sigure.tw/*"],  
            "js" : ["content.js"],  
            "css" : ["content.css"]  
        }  
    ],  
    "permissions" : ["activeTab"]  
}