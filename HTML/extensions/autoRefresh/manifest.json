{
    "manifest_version": 3,
  
    "name": "Automatical Refresh Webpage",
    "description": "This a test chrome extension",
    "version": "0.1.0",

    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_icon": {
        "16": "./icon.png",
        "48": "./icon.png",
        "128": "./icon.png"
      }
    },
    "permissions": [
      "activeTab"
    ]
  }