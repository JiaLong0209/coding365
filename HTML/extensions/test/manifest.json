{
    "manifest_version": 3,
  
    "name": "Move Page By Key",
    "description": "This a test chrome extension",
    "version": "0.11",

    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["content.js"]
      }
    ],

    "action": {  
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ]
  }